<!-- Key names are defined here: https://wiki.libsdl.org/SDL_Keycode -->
<keys>
	<key name="Left"   keycode="Left"/>
	<key name="Right"  keycode="Right"/>
	<key name="Up"     keycode="Up"/>
	<key name="Down"   keycode="Down"/>

	<key name="A"      keycode="X"/>
	<key name="B"      keycode="Z"/>

	<key name="Start"  keycode="Return"/>
	<key name="Select" keycode="Backspace"/>
</keys>
